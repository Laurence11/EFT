
<h2>Nous contacter</h2>

<form action="{{ path('bwf_contact') }}" method="post" {{ form_enctype(form) }}>
    {{ form_errors(form) }}

        {{ form_row(form.nom) }}
        {{ form_row(form.prenom) }}
        {{ form_row(form.email) }}
        {{ form_row(form.sujet) }}
        {{ form_row(form.message) }}

        {{ form_rest(form) }}

        <input type="submit" value="Envoyer" />
   
</form>

